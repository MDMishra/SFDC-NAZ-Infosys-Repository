        <fullName>Panel_Display__c</fullName>
        <externalId>false</externalId>
        <formula>Case(
  lower(KPI_Measurement__c),&quot;percent&quot;, TEXT(
  round(
   ABS(
    (IF(OR(ISBLANK(Sum_Denom__c),Sum_Denom__c = 0), 0 ,Sum_Num__c/Sum_Denom__c))*100
   ),0
  )
 ) &amp; &apos;%&apos;,
 &quot;xy&quot;, TEXT(Achieved_Targets__c) &amp; &apos;/&apos; &amp; TEXT(Count_of_SKUs__c),
&quot;&quot;)</formula>
